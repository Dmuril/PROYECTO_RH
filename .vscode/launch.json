{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run rrhh-employees",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/rrhh-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/projectemployees/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/employees",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/rrhh/employees\"}"
      },
      "envFile": "${workspaceFolder}/employees/.env2",
      "preLaunchTask": "Build rrhh"
    }
  ]
}